<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:style="http://schemas.android.com/apk/res-auto">


    <TextView
        android:id="@+id/textViewDev"
        style="@style/newStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/Developer"
        style:layout_constraintBottom_toBottomOf="parent"
        style:layout_constraintEnd_toEndOf="parent"
        style:layout_constraintHorizontal_bias="0.031"
        style:layout_constraintStart_toStartOf="parent"
        style:layout_constraintTop_toTopOf="parent"
        style:layout_constraintVertical_bias="0.016" />

    <TextView
        android:id="@+id/textViewAppName"
        style="@style/newStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/app_name"
        style:layout_constraintBottom_toBottomOf="parent"
        style:layout_constraintEnd_toEndOf="parent"
        style:layout_constraintHorizontal_bias="0.029"
        style:layout_constraintStart_toStartOf="parent"
        style:layout_constraintTop_toBottomOf="@+id/textViewDev"
        style:layout_constraintVertical_bias="0.026" />

    <TextView
        android:id="@+id/textViewVersion"
        style="@style/newStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/version"
        style:layout_constraintBottom_toBottomOf="parent"
        style:layout_constraintEnd_toEndOf="parent"
        style:layout_constraintHorizontal_bias="0.029"
        style:layout_constraintStart_toStartOf="parent"
        style:layout_constraintTop_toBottomOf="@+id/textViewAppName"
        style:layout_constraintVertical_bias="0.002" />
</android.support.constraint.ConstraintLayout>